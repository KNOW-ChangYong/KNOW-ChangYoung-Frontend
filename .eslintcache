rom \"../../actions/actions\";\nimport * as S from \"./styles\"\nimport { RootState } from \"typesafe-actions\"\n\nconst Navigator : React.FC = () => {\n    const navstate = useSelector((state: RootState) => (state.nav.state));\n    const dispatch=useDispatch();\n    useEffect(()=>{\n        console.log(navstate);\n    },[navstate])\n    return (\n        <S.NavigatorWrapper state={navstate}>\n            <S.NavigatorBT onClick={()=>dispatch(SetNav_Today())}>오늘</S.NavigatorBT>\n            <S.NavigatorBT onClick={()=>dispatch(SetNav_Private())}>전체</S.NavigatorBT>\n            <S.NavigatorBT onClick={()=>dispatch(SetNav_All())}>개인</S.NavigatorBT>\n        </S.NavigatorWrapper>\n    )\n};\nexport default Navigator;","/Users/an-eungyeol/Documents/project/KNOW-ChangYoung-Frontend/src/components/Main/List.tsx",["124","125","126","127","128","129","130"],"import axios from \"axios\";\nimport { useEffect, useState } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { RootState } from \"typesafe-actions\";\nimport { AttendanceResponse } from \"../../interfaces\";\nimport ListItem from \"./ListItem\";\nimport * as S from \"./styles\"\nconst List : React.FC = () => {\n    const [data, setData] = useState<AttendanceResponse[] | null>(null);\n    const [per,setPer] = useState(0);\n    const test=new Date().toString();\n    const navstate = useSelector((state: RootState) => (state.nav.state));\n    function getFormatDate(date: Date){\n        var year = date.getFullYear();\n        var month: number | string = (1 + date.getMonth());\n        month = month >= 10 ? month : '0' + month;\n        var day: number | string = date.getDate();\n        day = day >= 10 ? day : '0' + day;\n        return year + '-' + month + '-' + day;\n    }\n    async function getAttendance(){\n        let temp;\n        let total=0;\n        if(navstate==\"today\") temp=await axios.get(`https://changapi.eungyeol.live/attendance/${getFormatDate(new Date())}`)\n        else if(navstate==\"private\") temp=await axios.get(`https://changapi.eungyeol.live/attendance/profile/${localStorage.userId}`)\n        else temp=await axios.get(\"https://changapi.eungyeol.live/attendance\")\n        if(navstate==\"all\"){\n            temp.data.map((i : AttendanceResponse,index : number)=>{\n                if(i.attendanceCount && i.dateSum) total+=i.attendanceCount/i.dateSum;\n                if(index==20) setPer(total/21);\n            })\n        }\n        setData(temp.data)\n    }\n    useEffect(()=>{\n        getAttendance();\n    },[navstate])\n    return (\n        <S.ListWrapper>\n            <S.ListTitle>{navstate===\"today\" ? \"오늘\" : navstate===\"all\" ? \"전체\" : \"개인\"} …</S.ListTitle>\n            <S.ListTime>{test} {navstate===\"today\" ?  `| 출석 현황 : ${data?.length} / 21` : navstate==\"private\" ? `| 출석횟수 : ${data?.length}` : `| 출석률 : ${Number(per.toFixed(2))*100}%` }</S.ListTime>\n            <S.ListTable>\n                <thead>\n                    <tr>\n                        <th>프로필</th>\n                        <th>이름 / 계급</th>\n                        <th>{navstate===\"all\" ? \"출석 / 미출석\" : \"출석 시간\"}</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {\n                        data && data.map((i,index)=>(<ListItem key={index} data={i}></ListItem>))\n                    }\n                </tbody>\n            </S.ListTable>\n        </S.ListWrapper>        \n    )\n};\nexport default List;","/Users/an-eungyeol/Documents/project/KNOW-ChangYoung-Frontend/src/components/Main/Saying.tsx",[],"/Users/an-eungyeol/Documents/project/KNOW-ChangYoung-Frontend/src/components/Main/ListItem.tsx",["131","132"],"import { AttendanceResponse } from \"../../interfaces\";\nconst ListItem = ({data}: {data:AttendanceResponse})=>  {\n    return(\n        <tr>\n            <td>\n                <img src={require(`./userimages/${data.name}.jpg`).default}></img>\n            </td>\n            <td>\n        